<app-header title="Your Lists"></app-header>

<app-content-window>
    <app-list-manager {isfirst}="isFirst">

        {{#each taskLists}}
        <app-task-list {parent}="taskLists" {tasklist}="." {open}="false">

            {{#each tasks}}
            <app-task/>
            {{/each}}

        </app-task-list>
        {{/each}}

    </app-list-manager>
</app-content-window>

<app-utils></app-utils>
