<app-header title="Your Lists"></app-header>

<app-content-window>
    <app-list-manager taskLists="{taskLists}">

        {{#each taskLists}}
        <app-list>

            {{#each tasks}}
            <app-task/>
            {{/each}}

        </app-list>
        {{/each}}

    </app-list-manager>
</app-content-window>

<app-utils></app-utils>
