{{ log }} 
<ul class="task-list {{#isMostRecent}}active{{/isMostRecent}}">

    <li can-click="toggleOpen" class="task-list-title">
        <span>{{ title }}</span>
        <span class="task-list-chevron">
            <i class="fa fa-chevron-{{#is open 'true'}}down{{else}}right{{/is}}"/>
        </span>
    </li>

    {{#is open 'true'}}
    <content/>
    {{/is}}

</ul>
