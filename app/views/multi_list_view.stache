<app-header {view}='view'/>

<app-content-window>
    <app-list-manager>

        {{#each taskLists}}
        <app-task-list {listManager}="taskLists" {tasklist}="." {open}="false">

            {{#each tasks}}
            <app-task/>
            {{/each}}

        </app-task-list>
        {{/each}}

    </app-list-manager>
</app-content-window>

<app-dashboard {view}='view'/>
