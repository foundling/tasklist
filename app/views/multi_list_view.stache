<app-header {view}='view'/>

<app-content-window>
    <app-list-manager>

        {{#each taskLists}}
        <app-task-list {list-manager}="taskLists" {task-list}="." {open}="false">

            {{#each tasks}}
            <app-task {task}="." />
            {{/each}}

        </app-task-list>
        {{/each}}

    </app-list-manager>
</app-content-window>

<app-dashboard {view}='view'/>
