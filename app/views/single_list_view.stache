<app-content-window>
<app-list-manager {^overflow}="overflow">
    <app-task-list-single  {task-list}="data" open="true" >
        {{#each taskList.tasks }}
            <app-task task="." />
        {{/each}}
    </app-task-list-single>
</app-list-manager>
</app-content-window>
<app-dashboard {(settings-active)}="settingsActive" {overflow}="overflow" view="multilist"/>
